{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "completion",
    "description": "A completion of the current application",
    "type": "object",
    "patternProperties": {
        ".": {
            "title": "arguments",
            "description": "Arguments of the current application",
            "type": "array",
            "minItems": 1,
            "uniqueItems": true,
            "items": {
                "description": "An argument of the current application",
                "type": "string",
                "minLength": 1,
                "examples": [
                    "--help",
                    "--version",
                    "-h",
                    "-v",
                    "<alias>",
                    "<arrayvar>",
                    "<binding>",
                    "<builtin>",
                    "<command>",
                    "<directory>",
                    "<disabled>",
                    "<enabled>",
                    "<export>",
                    "<file>",
                    "<function>",
                    "<group>",
                    "<helptopic>",
                    "<hostname>",
                    "<job>",
                    "<keyword>",
                    "<running>",
                    "<service>",
                    "<signal>",
                    "<stopped>",
                    "<user>",
                    "<variable>"
                ]
            }
        }
    },
    "additionalProperties": false
}