{
    "$schema": "http://json-schema.org/draft-07/schema",
    "definitions": {
        "invalid-command-substitution": {
            "type": "string",
            "pattern": "\\$\\(\\s*\\)"
        },
        "completion": {
            "title": "completions",
            "description": "Completions of the current command or sub-command\nhttps://github.com/DannyBen/completely#using-the-completely-command-line",
            "type": "array",
            "minItems": 1,
            "uniqueItems": true,
            "items": {
                "description": "A completion of the current command or sub-command\nhttps://github.com/DannyBen/completely#using-the-completely-command-line",
                "type": "string",
                "minLength": 1,
                "not": {
                    "$ref": "#/definitions/invalid-command-substitution"
                },
                "examples": [
                    "--help",
                    "--version",
                    "-h",
                    "-v",
                    "--",
                    "<alias>",
                    "<arrayvar>",
                    "<binding>",
                    "<builtin>",
                    "<command>",
                    "<directory>",
                    "<disabled>",
                    "<enabled>",
                    "<export>",
                    "<file>",
                    "<function>",
                    "<group>",
                    "<helptopic>",
                    "<hostname>",
                    "<job>",
                    "<keyword>",
                    "<running>",
                    "<service>",
                    "<signal>",
                    "<stopped>",
                    "<user>",
                    "<variable>"
                ]
            }
        }
    },
    "title": "completion",
    "description": "A completion of the current application\nhttps://github.com/DannyBen/completely#using-the-completely-command-line",
    "type": "object",
    "properties": {
        "mygit checkout": {
            "$ref": "#/definitions/completion"
        },
        "mygit checkout*--branch": {
            "$ref": "#/definitions/completion"
        }
    },
    "patternProperties": {
        ".": {
            "$ref": "#/definitions/completion"
        }
    },
    "additionalProperties": false
}